{
  "parser": "vue-eslint-parser",
  "extends": ["plugin:vue/recommended"],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "overrides": [
    {
      "files": ["src/assets/iconfont/*.js"],
      "rules": {
        "no-self-assign": "off"
      }
    }
  ],
  "rules": {
    // 禁用多词组件名称规则
    "vue/multi-word-component-names": "off"
  }
}
